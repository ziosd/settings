{
  "name": "@gplatform/settings",
  "version": "1.2.2",
  "description": "gPlatform Production Settings",
  "main": "dist/index.js",
  "scripts": {
    "eslint": "eslint ./src",
    "test": "yarn run eslint && mocha ./**/*.test.js --require babel-core/register",
    "start": "node example/main.js",
    "build": "babel ./src --presets env --out-dir ./dist --ignore '**/*.test.js' --no-comments --minified"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ziosd/settings"
  },
  "keywords": [
    "gplatform",
    "settings",
    "config",
    "development",
    "production",
    "docker"
  ],
  "author": "gary.ascuy@gmail.com",
  "license": "MIT",
  "dependencies": {
    "joi": "^13.1.2",
    "lodash": "^4.17.5",
    "mpath": "^0.3.0"
  },
  "bin": {
    "settings": "dist/bin/cli.js"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "eslint": "^4.17.0",
    "eslint-plugin-mocha": "^4.11.0",
    "mocha": "^5.0.0"
  }
}
